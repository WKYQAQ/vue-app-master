.mapState(\"user\",[\"info\"])\n  },\n  created(){\n    this.loadOrders();\n  },\n  data(){\n    return {\n      active:0,\n      orders:[],\n      status:null\n    }\n  },\n  methods:{\n        // 点击tab的时候执行的回调函数\n    tabClickHandler(name,title){\n      // 修改当前data中status的值； 重新加载订单\n      this.status = title === \"全部\" ? null : title;\n      this.loadOrders();\n    },\n    loadOrders(){\n      let url=\"/order/query\";\n      let params={\n        customerId:this.info.id,\n        status:this.status\n      }\n      get(url,params).then((response)=>{\n        this.orders=response.data;\n      })\n\n    }\n  }\n}\n</script>\n<style scoped>\n.order {\n  background: #f1f1f1;\n}\n</style>"]}]}